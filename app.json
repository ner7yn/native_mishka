{
  "expo": {
    "name": "native_mishka",
    "slug": "native_mishka",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Loading.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Loading.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "loading": false
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Loading.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "FOREGROUND_SERVICE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "favicon": "./assets/Loading.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}